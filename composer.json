{
    "name": "devanoxltd/valet",
    "description": "A more enjoyable local development experience for Mac.",
    "keywords": ["laravel", "zonda", "wwdhhd"],
    "license": "MIT",
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        },
        {
            "name": "Adam Wathan",
            "email": "adam.wathan@gmail.com"
        },
        {
            "name": "Matt Stauffer",
            "email": "matt@tighten.co"
        }
    ],
    "autoload": {
        "files": [
            "cli/includes/compatibility.php",
            "cli/includes/facades.php",
            "cli/includes/helpers.php"
        ],
        "psr-4": {
            "Valet\\": "cli/Valet/"
        }
    },
    "autoload-dev": {
        "files": [
            "tests/UsesNullWriter.php",
            "tests/BaseApplicationTestCase.php",
            "tests/Drivers/BaseDriverTestCase.php"
        ]
    },
    "require": {
        "php": "^8.4",
        "illuminate/collections": "^11.0|^12.0",
        "illuminate/container": "^11.0|^12.0",
        "mnapoli/silly": "^1.9",
        "symfony/console": "^7.0|^8.0",
        "symfony/process": "^8.0",
        "guzzlehttp/guzzle": "^7.4",
        "symfony/event-dispatcher": "^7.0|^8.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.2.3",
        "yoast/phpunit-polyfills": "^4.0",
        "laravel/pint": "^1.4"
    },
    "bin": [
        "valet"
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "4.x-dev"
        }
    }
}
